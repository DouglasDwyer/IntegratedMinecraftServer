<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>IMS Plugin</Name>
    <Description>Creates a new project that can be used to develop IntegratedMinecraftServer extensions.</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>IMS Plugin</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.ico</Icon>
    <PreviewImage>__PreviewImage.ico</PreviewImage>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="IMS-DevelopmentKitTemplate.csproj" File="IMS-DevelopmentKitTemplate.csproj" ReplaceParameters="true">
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="launchSettings.json">launchSettings.json</ProjectItem>
      </Folder>
      <Folder Name="wwwroot" TargetFolderName="wwwroot" />
      <Folder Name="Library" TargetFolderName="Library">
        <ProjectItem ReplaceParameters="false" TargetFileName="IMS-Interface.dll">IMS-Interface.dll</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="IMS-Interface.xml">IMS-Interface.xml</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="IMS-Library.dll">IMS-Library.dll</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="IMS-Library.xml">IMS-Library.xml</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="Plugin.cs">Plugin.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>